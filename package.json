    "test:unit": "vitest",
    "e2e": "playwright test",
    "report": "pnpm exec playwright install",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "dev:server": "bun run --watch src/server/server.ts 3002"
  },
  "dependencies": {
    "pinia": "^3.0.1",
    "survey-core": "^1.12.25",
    "survey-vue3-ui": "^1.12.25",
    "vue": "^3.5.13"
